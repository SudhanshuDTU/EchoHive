{
  "expo": {
    "name": "EchoHive",
    "slug": "EchoHive",
    "scheme": "EchoHive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/final-image.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "assets/images/ECHOHIVE.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.sudhanshujha.echohive"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/final-image.jpg",
        "backgroundColor": "#000000",
        "resizeMode": "contain"
      },
      "package": "com.sudhanshujha.echohive"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2b1ca3e9-a598-4e16-855b-89495e238eb1"
      }
    }
  }
}
